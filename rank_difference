// Задано двузначное число. Найдите разность между количеством десятков и едениц.
//Asking for a two-digit number. Program finds the difference between the number of tens and ones.

#include <stdio.h>

int main ()
{ int a, b, c,d;
   puts ("Input a 2-digit number:");
   scanf("%i", &a);
   if(a>=100 || a<10)
   {
    puts("You better read first");
    return 0;
   }
    b=a/ 10;
    c= a%10;
    if (c>=b) {d=c-b;}
    if (c<b) {d=b-c;}
     
    printf ("%i", d);
   
   return 0;
 }

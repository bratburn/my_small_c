
// Велосипедист едет с постоянной скоростью v км/ч за сколько минут он проедет расстояние в S км?_//

#include <stdio.h>

int main ()
{ float t,v,s;
   puts ("Введите v:");
   scanf("%f", &v);
   puts ("Введите S:");
   scanf("%f", &s);
   t= (s/v)*60;
   printf ("Велосипедист проедет заданное расстояние за %f минут" , t);
   return 0;
 }
